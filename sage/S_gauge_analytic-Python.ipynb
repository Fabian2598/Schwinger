   "source": [
    "import numpy as np\n",
    "import scipy.special as sp\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(0, 8, 100)\n",
    "\n",
    "plt.plot(x, x * (1.0 - sp.i1(x) / sp.i0(x)))\n",
    "\n",
    "plt.show()"
   ]
